{
  "extends": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true
  },
  "rules": {
    "strict": [0],
    "func-names": [0],
    "no-param-reassign": [0],
    "global-require": [0],
    "no-console": [0],
    "import/no-dynamic-require": [0],
    "react/jsx-uses-react": "off",
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "never"        
      }
    ]
  }
}
